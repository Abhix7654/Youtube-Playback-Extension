{
        "manifest_version": 3,
        "name": "YouTube Speed Controller",
        "version": "1.0",
        "description": "Control YouTube playback speed with keyboard shortcuts.",
        "permissions": [
          "activeTab",
          "tabs"
        ],
        "content_scripts": [
          {
            "matches": ["*://*.youtube.com/*"],
            "js": ["content.js"]
          }
        ]
      }
      